  	file = {:home/rocio/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Signer, Balkenhol - 2015 - Reproducible home ranges (rhr) A new, user-friendly R package for analyses of wildlife telemetry data.pdf:pdf},
  	isbn = {1938-5463},
