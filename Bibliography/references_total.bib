  	journal = {Human Movement Science},
  	keywords = {Age-related experience,Compound positional variables,Entropy analysis,Practice task constraints,Small-sided games,Soccer},
  	pages = {74--81},
  	pmid = {27132155},
  	title = {{Age-related effects of practice experience on collective behaviours of football players in small-sided games}},
  	volume = {48},
